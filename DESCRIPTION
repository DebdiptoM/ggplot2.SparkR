Package: ggplot2.SparkR
Type: Package
Title: Big Data Visualization for R and Spark
Version: 1.0.6
Authors@R: c(
    person("Jong Hyun", "Bae", role = c("aut", "cre"), email = "jonghbae@skku.edu"),
    person("Moon Kyung", "Kim", role = "aut", email = "moonkyung1990@gmail.com"),
